<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="mattski47.databases386" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddDietActivity"
            android:label="@string/title_activity_add_diet" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewDietActivity" />
        </activity>
        <activity
            android:name=".AddExerciseActivity"
            android:label="@string/title_activity_add_exercise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewExerciseActivity" />
        </activity>
        <activity
            android:name=".AddSleepActivity"
            android:label="@string/title_activity_add_sleep" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".ViewSleepActivity" />
        </activity>
        <activity
            android:name=".ViewDietActivity"
            android:label="@string/title_activity_view_diet" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ViewExerciseActivity"
            android:label="@string/title_activity_view_exercise" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".ViewSleepActivity"
            android:label="@string/title_activity_view_sleep" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
    </application>
</manifest>